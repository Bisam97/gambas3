Dim Dashes As Float[] = [50.0,  'ink
                         10.0,  'skip
                         10.0,  'ink
                         10.0]  'skip
Paint.Dash = Dashes
Paint.DashOffset = -50
Paint.LineWidth = 10.0
Paint.MoveTo(128.0, 25.6)
Paint.LineTo(230.4, 230.4)
Paint.LineTo(230.4 + -102.4, 230.4)
Paint.CurveTo(51.2, 230.4, 51.2, 128.0, 128.0, 128.0)
Paint.Stroke
