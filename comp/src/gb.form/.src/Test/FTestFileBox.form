    MoveScaled(11,19,32,4)
    Title = ("Open image file")
    Filter = "*.png;*.jpg \"Image files\" *.ico \"Icon Files\""
    Picture = Picture["icon:/small/insert-image"]
    Placeholder = ("Image filters")
  }
}
