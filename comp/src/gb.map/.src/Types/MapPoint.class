Export
Public Lat As Float
Public Lon As Float

Public Sub _new(Optional Latitude As Float, Longitude As Float)
  
  Lat = Latitude
  Lon = Longitude
  
End

Static Public Function _Call(Latitude As Float, Longitude As Float) As MapPoint
  
