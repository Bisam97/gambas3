# Gambas Form File 3.0

{ Form Form
  MoveScaled(0,0,67,18)
  Background = Color.TextBackground
  Text = ("Find list")
  Icon = Picture["img/16/find-list.png"]
  Persistent = True
  Type = WindowType.Utility
  SkipTaskbar = True
  Arrangement = Arrange.Horizontal
  Spacing = True
  Padding = 4
  { panSearch Panel
    MoveScaled(1,1,14,15)
    { picSearch PictureBox
      MoveScaled(1,5,8,8)
      Picture = Picture["img/search.png"]
      Stretch = True
    }
    { btnCancel Button
      MoveScaled(0,0,14,4)
      Text = ("Close")
      Cancel = True
    }
    { timSearch #Timer
      #X = 60
      #Y = 30
      Delay = 100
    }
  }
  { lblNotFound TextLabel
    MoveScaled(19,1,39,12)
    Visible = False
    Font = Font["Bold,+2"]
    Expand = True
    Padding = 4
    Text = ("Search string cannot be found.")
    Border = Border.Sunken
  }
  { tvwFind GridView
    MoveScaled(19,1,47,16)
    Expand = True
    Mode = Select.Single
    Header = GridView.Horizontal
  }
}
