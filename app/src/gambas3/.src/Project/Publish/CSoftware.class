Static Public FarmServer As String

Private $iId As Integer

Static Private Sub DoRequest(sReq As String) As String

  Dim hHttpClient As HttpClient
  
  hHttpClient = New HttpClient
  hHttpClient.Async = False
  hHttpClient.URL = FarmServer &/ sReq
  
End



Public Sub _new(iSoftwareId As Integer)
  
  $iId = iSoftwareId
  
End
