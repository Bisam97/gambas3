# Gambas Form File 3.0

{ Form Form
  MoveScaled(0,0,102,84)
  Background = &HDF6B00&
  Foreground = &HFFFFFF&
  Text = ("Gambas Software Farm")
  Arrangement = Arrange.Vertical
  { dwgTitle DrawingArea
    MoveScaled(1,1,66,8)
    Background = &H3398C3&
  }
  { Panel1 VBox
    MoveScaled(1,11,97,17)
    Spacing = True
    Margin = True
    { panFilter Panel
      MoveScaled(2,1,94,4)
      Arrangement = Arrange.Horizontal
      Spacing = True
      { Label2 Label
        MoveScaled(1,0,7,4)
        AutoResize = True
        Text = ("Farm")
      }
      { cmbFarm ComboBox
        MoveScaled(10,0,29,4)
        ReadOnly = True
      }
      { Label1 Label
        MoveScaled(41,0,8,4)
        AutoResize = True
        Text = ("Show")
      }
      { cmbSort ComboBox
        MoveScaled(48,0,25,4)
        ReadOnly = True
        List = [("Most recent"), ("Most downloaded"), ("Most popular"), ("Installed")]
        Text = ("ComboBox1")
      }
      { Panel3 Panel
        MoveScaled(74,0,1,4)
        Expand = True
      }
      { btnLogin Button
        MoveScaled(77,0,15,4)
        Text = ("Connect") & "..."
      }
    }
    { tagFarm CTagEditor
      MoveScaled(1,6,95,4)
      Expand = True
    }
    { Panel2 Panel
      MoveScaled(1,11,95,4)
      Arrangement = Arrange.Fill
      Border = Border.Plain
      { txtFilter ButtonBox
        MoveScaled(0,0,31,4)
        Picture = Picture["icon:/small/clear"]
        Border = False
      }
    }
  }
  { Separator1 Separator
    MoveScaled(44,35,17,0)
  }
  { timLoad #Timer
    #MoveScaled(87,35)
    Delay = 250
  }
  { svwFarm ScrollView
    MoveScaled(3,45,90,37)
    Expand = True
    Arrangement = Arrange.Row
    Spacing = True
    Margin = True
    Border = False
    ScrollBar = Scroll.Vertical
  }
}
