Public Sub Run() As Boolean

  Return Not Me.ShowModal()

End

Public Sub btnOK_Click()

  Project.Config["/FImageQuality/Quality"] = sldQuality.Value
  Me.Close(True)

End

Public Sub btnCancel_Click()

  Me.Close

End


Public Sub Form_Open()

  sldQuality.Value = Project.Config["/FImageQuality/Quality", 80]

End
