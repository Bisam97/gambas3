# Gambas Form File 3.0

{ Form Form
  MoveScaled(18,23,56,40)
  Text = ("Save modified files")
  Icon = Picture["img/16/quit.png"]
  Border = Window.Fixed
  { btnSave Button
    MoveScaled(38,10,17,4)
    Text = ("&All")
    Picture = Picture["icon:/small/save"]
  }
  { btnQuit Button
    MoveScaled(38,30,17,4)
    Enabled = False
    Text = ("Continue")
    Picture = Picture["icon:/small/play"]
    Default = True
  }
  { btnCancel Button
    MoveScaled(38,35,17,4)
    Text = ("Cancel")
    Picture = Picture["icon:/small/close"]
    Cancel = True
  }
  { Panel1 Panel
    MoveScaled(1,1,52,9)
    { TextLabel1 TextLabel
      MoveScaled(6,0,45,6)
      Font = Font["Bold,+1"]
      Text = ("These files have been modified.\nDo&nbsp;you want to save them?")
      Alignment = Align.Normal
    }
    { Image1 PictureBox
      MoveScaled(0,1,5,5)
      Picture = Picture["icon:/32/warning"]
      Alignment = Align.TopNormal
    }
  }
  { lvwFile ListView
    MoveScaled(1,10,36,29)
    Mode = Select.Multiple
  }
  { btnIgnore Button
    MoveScaled(38,15,17,4)
    Text = ("&None")
    Picture = Picture["icon:/small/cancel"]
  }
}
